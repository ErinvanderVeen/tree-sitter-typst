{
  "name": "typst",
  "word": "ident",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_markup_expr"
      }
    },
    "_markup_expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "whitespace"
        },
        {
          "type": "SYMBOL",
          "name": "parbreak"
        },
        {
          "type": "SYMBOL",
          "name": "line_comment"
        },
        {
          "type": "SYMBOL",
          "name": "block_comment"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "linebreak"
        },
        {
          "type": "SYMBOL",
          "name": "escape"
        },
        {
          "type": "SYMBOL",
          "name": "shorthand"
        },
        {
          "type": "SYMBOL",
          "name": "smart_quote"
        },
        {
          "type": "SYMBOL",
          "name": "raw"
        },
        {
          "type": "SYMBOL",
          "name": "link"
        },
        {
          "type": "SYMBOL",
          "name": "label"
        }
      ]
    },
    "linebreak": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\r\\n|[\\n\\x0B\\x0C\\r\\x85\\u2028\\u2029]"
              },
              {
                "type": "PATTERN",
                "value": "\\s"
              }
            ]
          }
        ]
      }
    },
    "escape": {
      "type": "PATTERN",
      "value": "\\\\u\\{[a-fA-F0-9]{1,4}\\}"
    },
    "shorthand": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "..."
          },
          {
            "type": "STRING",
            "value": "---"
          },
          {
            "type": "STRING",
            "value": "--"
          },
          {
            "type": "STRING",
            "value": "-?"
          },
          {
            "type": "STRING",
            "value": "~"
          }
        ]
      }
    },
    "smart_quote": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "'"
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "link": {
      "type": "PATTERN",
      "value": "https?:\\/\\/[0-9a-zA-Z~\\/%?#&+='\\.,;]*[0-9a-zA-Z~\\/%?#&+=',;]"
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "PATTERN",
            "value": "[-_\\p{XID_Continue}]+"
          }
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "ident": {
      "type": "PATTERN",
      "value": "[_\\p{XID_Start}][-_\\p{XID_Continue}]*"
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "block_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/*"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PREC",
                "value": 1,
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "line_comment"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "block_comment"
                    }
                  ]
                }
              },
              {
                "type": "PATTERN",
                "value": "."
              },
              {
                "type": "PATTERN",
                "value": "\\r\\n|[\\n\\x0B\\x0C\\r\\x85\\u2028\\u2029]"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "*/"
            },
            {
              "type": "SYMBOL",
              "name": "eof"
            }
          ]
        }
      ]
    },
    "whitespace": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "[ \\t]+"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "PATTERN",
                    "value": "\\r\\n|[\\n\\x0B\\x0C\\r\\x85\\u2028\\u2029]"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "[ \\t]*"
              },
              {
                "type": "PATTERN",
                "value": "\\r\\n|[\\n\\x0B\\x0C\\r\\x85\\u2028\\u2029]"
              }
            ]
          }
        ]
      }
    },
    "parbreak": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[ \\t]*"
          },
          {
            "type": "PATTERN",
            "value": "\\r\\n|[\\n\\x0B\\x0C\\r\\x85\\u2028\\u2029]"
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "PATTERN",
              "value": "\\r\\n|[\\n\\x0B\\x0C\\r\\x85\\u2028\\u2029]"
            }
          }
        ]
      }
    }
